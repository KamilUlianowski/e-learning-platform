@model List<E_LearningWeb.Core.Models.User>
@if (Model.Count > 0)
{
    <div class="row score-row">
        <h2>
            <span>1.</span>
            <label>@Model[0].Name</label>
            <span>@Model[0].CorrectAnswers</span>
        </h2>
    </div>
}
@for (int i = 1; i < Model.Count; i++)
{
    <div class="row score-row">
        <h3>
            <span>@(i + 1).</span>
            <label>@Model[i].Name</label>
            <span>@Model[i].CorrectAnswers</span>
        </h3>
    </div>
}
